    "tslib": "2.6.2",
    "zone.js": "0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.7",
    "@angular/cli": "17.3.7",
    "@angular/compiler-cli": "17.3.8",
    "@types/jasmine": "5.1.4",
    "@types/jest": "29.5.12",

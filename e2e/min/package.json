{
  "name": "min",
  "description": "Angular Minimal Dependencies",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "npm": "npm",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run test:jasmine && npm run test:jest",
    "test:debug": "npm run test:jasmine:debug",
    "test:jasmine": "ng test",
    "test:jasmine:debug": "npm run test:jasmine -- --browsers=Chrome --watch",
    "test:jest": "jest -w 1 --config jest.config.ts",
    "test:jest:debug": "npm run test:jest -- --watch"
  },
  "peerDependencies": {
    "ng-mocks": "*"
  },
  "dependencies": {
    "@angular/common": "15.1.4",
    "@angular/compiler": "15.1.4",
    "@angular/core": "15.1.4",
    "@angular/forms": "15.1.4",
    "@angular/platform-browser": "15.1.4",
    "@angular/platform-browser-dynamic": "15.1.4",
    "@angular/router": "15.1.4",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.5",
    "@angular/cli": "15.1.5",
    "@angular/compiler-cli": "15.1.4",
    "@types/jasmine": "4.3.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "29.4.2",
    "jest-preset-angular": "13.0.0-next.1",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "15.1.2",
    "npm": "9.5.0",
